{
  "name": "lin3s/wordpress-api-client",
  "description": "WordPress API client for WP-API v2",
  "keywords": ["lin3s", "wordpress", "client", "api", "wp-api"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "LIN3S",
      "email": "info@lin3s.com",
      "homepage": "https://lin3s.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "guzzlehttp/guzzle": "^6.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^1.12"
  },
  "scripts": {
    "cs": [
      "php-cs-fixer fix --config-file=.php_cs"
    ]
  },
  "autoload": {
    "psr-4": {
      "LIN3S\\WordPressApiClient\\": "src/WordPressApiClient/"
    }
  }
}
